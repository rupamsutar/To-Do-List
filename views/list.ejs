<%- include("header") -%>


    <div class="box" id="heading">
      <h1><%= listItems %></h1>
    </div>

    <div class="box">
        <% for (var i = 0; i < itemsx.length ; i++) { %>
          <div class="item">
            <input type="checkbox">
            <p><%= itemsx[i] %> </p>
          </div>
        <% } %>

        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="New Item" autocomplete = "off">
          <button type="submit" name="list" value=<%= listItems %>>+</button>
        </form>
    </div>


<%- include("footer") -%>
